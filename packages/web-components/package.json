    "@sveltejs/vite-plugin-svelte": "^6.2.4",
    "@tailwindcss/vite": "^4.1.18",
    "@types/node": "^25.0.9",
    "@uoguelph/tailwind-theme": "*",
    "autoprefixer": "^10.4.23",
    "eslint": "^9.39.2",
    "eslint-plugin-svelte": "^2.46.1",
    "postcss": "^8.5.6",
    "svelte": "^5.47.1",
    "svelte-check": "^4.3.5",
    "tailwind-merge": "^3.4.0",
    "tailwind-variants": "^3.2.2",
    "tailwindcss": "^4.1.18",
    "vite": "^7.3.1",
    "vite-plugin-svgo": "^2.0.0"
  },
